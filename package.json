{
  "name": "@alex7kom/sayonara-config",
  "version": "0.9.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Zero config tools",
  "bin": {
    "sayonara-config": "src/run.js"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{j,t}s?(x)\"",
    "pretty": "pretty-quick",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Alexey Komarov <alex7kom@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "confusing-browser-globals": "^1.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "eslintConfig": {
    "extends": [
      "./src/configs/eslint-node.js",
      "./src/configs/eslint-jest-overrides.js"
    ],
    "overrides": [
      {
        "files": [
          "*.tpl.js"
        ],
        "rules": {
          "node/no-missing-require": "off"
        }
      }
    ],
    "rules": {
      "jsdoc/require-jsdoc": "off"
    }
  },
  "devDependencies": {
    "husky": "^4.2.3"
  },
  "sayonaraConfig": {
    "sets": [
      "node-app",
      "npm-package"
    ]
  }
}
